        <td>
            @Html.DisplayFor(modelItem => item.NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PasswordUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreadoEn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblTipo.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idUsuario }) |
